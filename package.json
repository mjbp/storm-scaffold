{
    "name": "storm-scaffold",
    "version": "1.7.1",
    "description": "Project scaffold",
    "repository": "https://github.com/stormid/storm-scaffold",
    "author": "stormid",
    "license": "ISC",
    "scripts": {
        "start": "node_modules/.bin/gulp serve",
        "production": "node_modules/.bin/gulp serve --production",
        "compile": "node_modules/.bin/gulp compile --production",
        "ci": "npm run test && npm run compile",
        "watch": "node_modules/.bin/gulp watch",
        "test": "node_modules/.bin/jest"
    },
    "browserlist": [
        "ie >= 9",
        "ie_mob >= 10",
        "ff >= 20",
        "chrome >= 4",
        "safari >= 7",
        "opera >= 23",
        "ios >= 7",
        "android >= 4.4",
        "bb >= 10"
    ],
    "jest": {
        "coverageDirectory": "./coverage/",
        "collectCoverage": true,
        "collectCoverageFrom": [
            "./src/js/**",
            "!./src/js/async/**"
        ]
    },
    "devDependencies": {
        "babel-core": "^6.26.3",
        "babel-jest": "^24.7.1",
        "babel-preset-env": "^1.7.0",
